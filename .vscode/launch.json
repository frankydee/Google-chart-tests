    "version": "0.2.0",
    "configurations": [
     
        {
            "name": "Attach Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "url": "http://127.0.0.1:5500/index.html",
            "webRoot": "${workspaceRoot}"
        },
     
        {
            "type": "chrome",
            "request": "launch",
            "port": 9222,
            "name": "Launch Chrome",
            "url": "http://127.0.0.1:5500/index.html",
            "webRoot": "${workspaceRoot}"
        }


        {
            "type": "chrome",
            "request": "launch",
            "port": 9222,
            "name": "Launch Chrome so1",
            "url": "http://127.0.0.1:5500/so1.html",
            "webRoot": "${workspaceRoot}"
        }

    ]
}